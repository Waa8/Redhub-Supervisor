{
  "name": "comprehensive-productivity-app",
  "version": "1.0.0",
  "description": "Advanced Productivity App with Order Management, Logistics, Warehouse, Financial, and Customer Service Modules",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "node start-production.js",
    "start:cluster": "ENABLE_CLUSTERING=true node start-production.js",
    "dev": "nodemon server.js",
    "dev:prod": "NODE_ENV=production nodemon server-production.js",
    "build": "npm run build:client",
    "build:client": "webpack --mode production",
    "dev:client": "webpack serve --mode development",
    "deploy": "chmod +x scripts/deploy-production.sh && ./scripts/deploy-production.sh deploy pm2",
    "deploy:docker": "chmod +x scripts/deploy-production.sh && ./scripts/deploy-production.sh deploy docker",
    "test": "jest",
    "test:prod": "NODE_ENV=production jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "security:audit": "npm audit --audit-level moderate",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs productivity-app",
    "docker:up": "docker-compose -f docker-compose.production.yml up -d",
    "docker:down": "docker-compose -f docker-compose.production.yml down"
  },
  "keywords": [
    "productivity",
    "order-management",
    "logistics",
    "warehouse",
    "financial",
    "customer-service",
    "arabic",
    "english"
  ],
  "author": "Productivity App Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3",
    "rate-limiter-flexible": "^4.0.1",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "stripe": "^14.5.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.52.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.17.0",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
